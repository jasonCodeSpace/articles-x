{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "crons": [
    {
      "path": "/api/fetch-timeline",
      "schedule": "0,12,24,36,48 * * * *"
    },
    {
      "path": "/api/process-articles",
      "schedule": "6,30,54 * * * *"
    },
    {
      "path": "/api/generate-summaries",
      "schedule": "8,32,56 * * * *"
    },
    {
      "path": "/api/cleanup-tweets",
      "schedule": "0 2 */2 * *"
    },
    {
      "path": "/api/search-articles",
      "schedule": "15 2 1 */3 *"
    },
    {
      "path": "/api/update-article-tags",
      "schedule": "0 * * * *"
    }
  ]
}